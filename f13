#!/bin/sh
#  __| _ \ _ _|  _ \    \ \ \  /   __ __| |  |  __|   _ | __ / __ __| |  |
#  _|    /   |   |  |  _ \ \  /       |   __ |  _|      |  _ \    |   __ |
# _|  _|_\ ___| ___/ _/  _\ _|       _|  _| _| ___|    _| ___/   _|  _| _|
#

y1="${1:-$(date +%Y)}"
y2="${2:-$y1}"

for i in $(seq "$y1" "$y2"); do
  for j in $(seq 1 12); do
    test "$(date -d "$i-$j-13" +%a)" = 'Fri' && \
      printf '%04d-%02d-13\n' "$i" "$j"
  done
done
